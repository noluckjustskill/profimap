  "name": "ProfiMap",
  "version": "1.1.0",
  "description": "Map your skills",
  "author": "Artem Eskov",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
